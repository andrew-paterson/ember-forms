{{#each formField.radioButtons as |option|}}
  {{custom-elements/radio-button-element
    data-test-id=(concat 'radio-button-option-' option.value)
    value=option.value
    groupValue=groupValue
    required=false
    name=name
    disabled=disabled
    label=option.label
    changedAction=(action onUserInteraction groupValue)
    fieldName=option.fieldName
  }}
{{/each}}
