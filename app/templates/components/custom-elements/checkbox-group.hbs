<div class="checkbox-group-container">
  {{extensible/form-field-legend formField=formField}}
  {{component inputIcon class='form-field-icon'}}
  {{#each formField.options as |option|}}
    {{custom-elements/labelled-checkbox
      value=(readonly option.value)
      label=option.label
      changedAction=(action 'test' option.key)
      name=formField.name
    }}
  {{/each}}
</div>
{{extensible/form-field-error formField=formField}}